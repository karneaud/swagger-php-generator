{
	"name": "karneaud/swagger-php-generator",
    "license": "MIT",
	"type":"library",
	"version":"1.0.0-alpha",
	"authors": [
		{
			"name": "karneaud",
			"email": "kendall.arneaud@gmail.com"
		}
	],
	"require": {
		"php":">=7.4 || ^8.0",
		"nette/php-generator": "^3.0",
		"symfony/yaml": "^3.4",
		"symfony/serializer": "^3.4",
		"psr/http-message": "^1.0"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"phpunit/phpunit": "^9.0"
	},
	"autoload": {
		"psr-4": {
			"SwaggerGen\\": "src/"
		}
	},
	"autoload-dev":{
		"psr-4": {
			"SwaggerGen\\Tests\\":"tests/"
		}
	},
	"scripts": {
		"generate" : "./bin/generate",
		"test":"phpunit"
	},
	"bin":["bin/generate"]
}
